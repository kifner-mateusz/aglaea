{
  "name": "aglaea",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
 "json-server": "^0.15.1"
  },
  "scripts": {
    "start-json-server": "json-server --watch ./json_server/db.json --port 3001 --static ./json_server/static -S ./json_server",
    "start-json-server-slow": "json-server --watch ./json_server/db.json --port 3001 --static ./json_server/static -S ./json_server --delay 1000"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
