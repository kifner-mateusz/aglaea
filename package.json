{
  "name": "aglaea",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "json-server": "^0.15.1",
    "nodemon": "^1.19.4"
  },
  "scripts": {
    "start": "nodemon server.js",
    "start-json-server": "json-server --watch ./json_server/db.json  --static ./json_server/static -S ./json_server",
    "start-json-server-slow": "json-server --watch ./json_server/db.json  --static ./json_server/static -S ./json_server --delay 1000"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": "10.x"
  },
  "description": "Online shop mockup for statup",
  "repository": {
    "type": "git",
    "url": "git+https://kifner-mateusz@github.com/kifner-mateusz/aglaea.git"
  },
